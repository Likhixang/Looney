http-response ^https:\/\/m5-zb\.amap\.com\/ws\/boss\/(car\/order\/content_info|order_web\/friendly_information)\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove the taxi page promotion
http-response ^https:\/\/m5-zb\.amap\.com\/ws\/promotion-web\/resource(\/home)?\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove the taxi page promotion
http-response ^https:\/\/m5-zb\.amap\.com\/ws\/sharedtrip\/taxi\/order_detail_car_tips\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove the taxi page promotion

[Mitm]
hostname = m5.amap.com, m5-zb.amap.com, oss.amap.com, sns.amap.com