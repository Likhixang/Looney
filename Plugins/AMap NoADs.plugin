http-response ^https:\/\/m5\.amap\.com\/ws\/shield\/search\/(common\/coupon\/info|poi\/detail)\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove annoying components from navigation details page
http-response ^https:\/\/m5\.amap\.com\/ws\/shield\/search_business\/process\/marketingOperationStructured\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove annoying components from navigation details page
http-response ^https:\/\/m5\.amap\.com\/ws\/shield\/search_poi\/(homepage|mps|search\/sp|sug|tips_operation_location)\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove annoying components from navigation details page

http-response ^https:\/\/m5\.amap\.com\/ws\/faas\/amap-navigation\/(card-service-plan-home|main-page)\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove annoying components from the home page
http-response ^https:\/\/m5\.amap\.com\/ws\/shield\/frogserver\/aocs\/updatable\/1\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove annoying components from the home page

http-response ^https:\/\/m5\.amap\.com\/ws\/shield\/dsp\/profile\/index\/nodefaasv3\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove annoying components from my page

http-response ^https:\/\/m5\.amap\.com\/ws\/shield\/search\/nearbyrec_smart\? script-path = https://raw.githubusercontent.com/Likhixang/Looney/main/JavaScripts/Amap_remove_ads.js, requires-body = true, tag = Remove nearby page promotion

